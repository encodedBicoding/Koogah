  "main": "./build/WebSockets/socket.io.js",
  "scripts": {
    "test": "export NODE_ENV=test && npm run seed:undo && npm run seed && nyc mocha --timeout 10000 --require @babel/register server/api/v1/tests/* --exit",
    "test:package": "export NODE_ENV=test && nyc mocha --timeout 10000 --require @babel/register server/api/v1/tests/package.test.js --exit",
    "pretest": "export NODE_ENV=test && npm run migrate:undo && npm run migrate && npm run seed:undo && npm run seed",
    "start:dev": "export NODE_ENV=development && nodemon --exec babel-node ./server/WebSockets/socket.io.js",
    "build": "npm run clean && npm run migrate:undo && npm run migrate && babel -d ./build ./server",
    "clean": "rm -rf build && mkdir build",
    "prestart": "npm run migrate:undo && npm run migrate",
    "start": "export NODE_ENV=production && node ./build/WebSockets/socket.io.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "start:redis": "brew services start redis && redis-cli"
  },
